{
  "kind": "collectionType",
  "collectionName": "curated_channels",
  "info": {
    "singularName": "curated-channel",
    "pluralName": "curated-channels",
    "displayName": "Curated Channel"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "img": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "string",
      "required": true
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "curated_channels"
    },
    "url": {
      "type": "string",
      "required": true
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "art",
        "fashion",
        "food",
        "music",
        "video",
        "desgin"
      ],
      "required": true
    }
  }
}
